generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model goals {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  category String
  title    String
}

model users {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String
}
